export {monoid, mempty, +-monoid-u64, ×-monoid-u64}

record monoid
  (a : type) :=

  constructor monoid
  val mempty : a

open {{monoid}}

/--
  Additive monoid on unsigned 64-bis integer.
-/
let +-monoid-u64 
  : monoid u64 :=
  
  monoid 0 

/--
  Multiplicative monoid on unsigned 64-bis integer.
-/
let ×-monoid-u64 
  : monoid u64 :=

  monoid 1
